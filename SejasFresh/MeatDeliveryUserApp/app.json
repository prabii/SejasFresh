{
  "expo": {
    "name": "Sejas Fresh",
    "slug": "MeatDeliveryUserApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "meatdeliveryuserapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.batman1428.MeatDeliveryUserApp",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses notifications to keep you informed about your orders and offers.",
        "NSLocationWhenInUseUsageDescription": "This app needs your location to auto-fill your address for faster delivery.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to auto-fill your address for faster delivery."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.batman1428.MeatDeliveryUserApp",
      "versionCode": 2,
      "allowBackup": false,
      "blockedPermissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs your location to auto-fill your address for faster delivery."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#D13635",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "productionApiUrl": "https://meat-delivery-backend.onrender.com/api",
      "productionApiHost": "meat-delivery-backend.onrender.com",
      "useLocalBackend": "false",
      "apiPort": "5000",
      "apiHost": "192.168.1.5",
      "eas": {
        "projectId": "01c9f98b-2648-4a98-ae4d-b6a9dee68d1b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
